    "compilerOptions": {
        "target": "ES2020",
        "lib": ["ES2020"],
        "module": "CommonJS",
        "moduleResolution": "Node",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "downlevelIteration": true,
        "strict": true,
        "skipLibCheck": true,
        "types": ["node"]
    }
}